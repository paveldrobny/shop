Height =\n        document.body.scrollHeight - document.documentElement.clientHeight;\n\n    let percentValue = (scrollStart / scrollHeight) * 100;\n    setPercentage(percentValue);\n  }\n\n  return (\n    <header id=\"header\" className={headerActive}>\n      <ProgressBar value={percentage} type={\"headerV\"} />\n      <div className=\"header-cont\">\n        <div className=\"header-title\">Shop</div>\n        <div className=\"header-navBtn\">\n          <ul>\n            {navLinks.map((link) => {\n              return (\n                <li key={link.name}>\n                  <NavLink\n                    exact\n                    to={link.path}\n                    className=\"App-link\"\n                    activeClassName=\"selected\"\n                  >\n                    <i className={link.FAClass}></i>\n                  </NavLink>\n                  <div className=\"linkMessage\">{link.name}</div>\n                </li>\n              );\n            })}\n          </ul>\n        </div>\n      </div>\n    </header>\n  );\n}\n\nexport default Header;\n","C:\\Users\\Pavel\\Desktop\\reactshop\\src\\components\\ProgressBar\\ProgressBar.js",[],"C:\\Users\\Pavel\\Desktop\\reactshop\\src\\components\\Search\\Search.js",[],"C:\\Users\\Pavel\\Desktop\\reactshop\\src\\components\\Footer\\Footer.js",[],"C:\\Users\\Pavel\\Desktop\\reactshop\\src\\components\\Card\\Card.js",[],"C:\\Users\\Pavel\\Desktop\\reactshop\\src\\pages\\Product.js",[],"C:\\Users\\Pavel\\Desktop\\reactshop\\src\\components\\InfoProduct\\InfoProduct.js",[],"C:\\Users\\Pavel\\Desktop\\reactshop\\src\\pages\\Admin.js",[],"C:\\Users\\Pavel\\Desktop\\reactshop\\src\\components\\AdminPanel\\AddProductForm\\AddProductForm.js",["171"],"import React, { useState } from \"react\";\r\nimport \"./AddProductForm.css\";\r\n\r\nfunction AddProductForm({ handleChange, value }) {\r\n  return (\r\n    <div className=\"admin-input-main\">\r\n      <div className=\"admin-input-content\">\r\n        <label>Product 